<Window x:Class="ViewPane2App.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:viewPane="clr-namespace:ViewPane;assembly=ViewPane3"
    Title="Window1" Height="600" Width="900" Closed="Window_Closed" Closing="Window_Closing">
   
	<DockPanel>
		<Grid Width="100" DockPanel.Dock="Left">
			<StackPanel Background="#FFA0B0F0">
				<Button Width="75" Height="23" Content="Load" Margin="4" Click="Load_Click"></Button>
				<Button x:Name="buttonResults" IsEnabled="False" Width="75" Height="23" Content="Results" Margin="4" Click="Results_Click"></Button>
				<Button Width="75" Height="23" Content="Dlg" Margin="4" Click="Dlg_Click"></Button>
				<Button Width="75" Height="23" Content="Independent" Margin="4" ></Button>
				<Button Width="75" Height="23" Content="Dependent" Margin="4" ></Button>
				<Button Width="75" Height="23" Content="Test 1" Margin="4" Click="Test1_Click" ></Button>
				<Button Width="75" Height="23" Content="Test 2" Margin="4" Click="Test2_Click" ></Button>
				<Button Width="75" Height="23" Content="Test 3" Margin="4" Click="Test3_Click"></Button>
				<Button Width="75" Height="23" Content="Test 4" Margin="4" Click="Test4_Click"></Button>
				<Button Width="75" Height="23" Content="Test 5" Margin="4" Click="Test5_Click"></Button>
			</StackPanel>
		</Grid>

		<DockPanel>
			<viewPane:ViewPanel x:Name="viewPanel"/>
		</DockPanel>
	</DockPanel>
	
</Window>
